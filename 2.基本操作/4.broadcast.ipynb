   "source": [
    "import torch\n",
    "\n",
    "#broadcast 将不同形状的tensor进行计算，自动拓展低维度\n",
    "\n",
    "a = torch.randn(2, 3)\n",
    "b = torch.randn(1, 3)\n",
    "c = torch.randn(1)\n",
    "\n",
    "print((a + b).shape)\n",
    "print((a + c).shape)\n",
    "\n",
    "#手动boradcast\n",
    "print(b.expand_as(a).shape)\n",
    "print(c.expand_as(a).shape)"
   ]
